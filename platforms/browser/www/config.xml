<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.bookyourdoc" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Book Your Doc</name>
    <description>
        Encuentra tu doctor.
    </description>
    <author email="lromero@icone-solutions.com" href="https://icone-solutions.com">
        ICONE Soluciones
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access browserOnly="true" origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="stay-in-webview" value="false" />
</widget>
